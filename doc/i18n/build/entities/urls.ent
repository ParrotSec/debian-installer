<!ENTITY url-sublevel2-po     "&url-status;/level1/files/&language;/packages_po_sublevel2_&language;.po">
<!ENTITY url-sublevel3-po     "&url-status;/level1/files/&language;/packages_po_sublevel3_&language;.po">
