  Standard Debian URLs
  -->

<!ENTITY salsa "salsa.debian.org">
<!ENTITY url-salsa "https://&salsa;/">
<!ENTITY salsa-account-page '<ulink url="https://salsa.debian.org/public">https://salsa.debian.org/public</ulink>'>

<!ENTITY salsa-debian-auth "&lt;salsa_username&gt;@salsa.debian.org">
<!ENTITY salsa-debian "salsa.debian.org">

